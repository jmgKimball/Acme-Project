<aura:component implements="force:lightningQuickActionWithoutHeader,forceCommunity:availableForAllPageTypes,forceCommunity:layout" access="global" >
    
    <!--<lightning:notificationsLibrary aura:id="notifLib" />
 This is a form to create new application
    <lightning:recordForm objectApiName="Application__c" layoutType="Full" columns="2" onerror="{!c.handleError}" /> -->
    
    
    
    <aura:attribute name="newApplication" type="Object"/>
    <aura:attribute name="simpleNewApplication" type="Object"/>
    <aura:attribute name="newApplicationError" type="String"/>
    <aura:attribute name="cbaseURL" type="String"/>
    
    <force:recordData aura:id="applicationRecordCreator" layoutType="FULL" targetRecord="{!v.newApplication}" targetFields="{!v.simpleNewApplication}" targetError="{!v.newApplicationError}" />
    
   
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <lightning:recordEditForm objectApiName="Application__c" aura:id="applicationRecordSave" onsubmit ="{!c.handleSubmit}"> 
