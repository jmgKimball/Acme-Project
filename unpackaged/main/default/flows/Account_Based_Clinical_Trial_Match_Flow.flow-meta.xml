    <dynamicChoiceSets>
        <name>recordChoiceSetClinicalTrials</name>
        <dataType>String</dataType>
        <displayField>Brief_Title__c</displayField>
        <filters>
            <field>Clinical_Trial_Status__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Recruiting</stringValue>
            </value>
        </filters>
        <filters>
