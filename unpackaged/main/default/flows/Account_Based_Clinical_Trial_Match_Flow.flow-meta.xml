            <field>Id</field>
        </outputAssignments>
        <valueField>Id</valueField>
    </dynamicChoiceSets>
    <formulas>
        <name>fomulaClinicalTrialIDinExistingClinicalTrialPatientRecordList</name>
        <dataType>Boolean</dataType>
        <expression>CONTAINS({!stringClinicalTrialPatientRecords},{!loopingOriginalClinicalTrials.Id})</expression>
    </formulas>
    <formulas>
        <name>formulaClinicalTrialPatientIdAndSep</name>
        <dataType>String</dataType>
        <expression>{!loopingClinicalTrialPatient.Clinical_Trial__c} + &quot;;&quot;</expression>
    </formulas>
    <formulas>
        <name>formulaLengthOfChoiceSet</name>
        <dataType>Number</dataType>
        <expression>len({!recordChoiceSetClinicalTrials})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>formulaPatientAge</name>
        <dataType>Number</dataType>
        <expression>IF (ISNULL ({!get_Person_Contact_Record.Age_Num__c}), 1000, {!get_Person_Contact_Record.Age_Num__c})</expression>
        <scale>0</scale>
    </formulas>
