    <formulas>
        <name>firstSelectedSubCategory</name>
        <dataType>String</dataType>
        <expression>if(CONTAINS({!stringSelectedSubCat},&quot;;&quot;),
TRIM(LEFT({!stringSelectedSubCat}, FIND(&quot;;&quot;, {!stringSelectedSubCat})-1)),
{!stringSelectedSubCat})</expression>
    </formulas>
    <formulas>
        <name>lenStringSubCats</name>
        <dataType>Number</dataType>
        <expression>len({!stringSelectedSubCat})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
