        <expression>{!Community_Topics}</expression>
    </formulas>
    <formulas>
        <name>trimmedStringSelectedSubCat</name>
        <dataType>String</dataType>
        <expression>IF(CONTAINS({!stringSelectedSubCat},&quot;;&quot;),
TRIM(RIGHT({!stringSelectedSubCat}, LEN({!stringSelectedSubCat}) - FIND(&quot;;&quot;, {!stringSelectedSubCat}))),
&quot;&quot;)</expression>
    </formulas>
