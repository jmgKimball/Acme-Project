        </rules>
        <rules>
            <name>No_Mediation_Statement_Record</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Med_Statement.HealthCloudGA__Account__c</leftValueReference>
